{
  "name": "swing-dance-seattle",
  "version": "4.0.0",
  "description": "Schedule of swing dance experiences in Seattle",
  "main": "lib/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\" \"firebase serve\"",
    "clean:server": "rimraf lib",
    "clean:client": "rimraf public",
    "dev:server": "npm run clean:server && babel src --watch --out-dir lib",
    "dev:client": "npm run clean:client && webpack --watch --config webpack.dev.config.js",
    "build:server": "npm run clean:server && babel src --out-dir lib",
    "build:client:qa": "npm run clean:client && webpack --config webpack.qa.config.js",
    "build:client:prod": "npm run clean:client && webpack --config webpack.prod.config.js",
    "deploy:qa": "npm run build:server && npm run build:client:qa && firebase use qa && firebase deploy",
    "deploy:prod": "npm run build:server && npm run build:client:prod && firebase use production && firebase deploy",
    "seed:qa": "firebase use qa && firebase database:set / seed.json",
    "lint": "eslint src",
    "precommit": "npm run lint"
  },
  "author": "Stefan Huynh",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-formidable": "^3.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "firebase-tools": "^3.17.6",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-manifest-plugin": "^2.0.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "0.0.18",
    "classnames": "^2.2.5",
    "debounce": "^1.1.0",
    "detect-node": "^2.0.4",
    "firebase": "^4.12.0",
    "firebase-admin": "^5.12.0",
    "firebase-functions": "^1.0.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reset-css": "^3.0.0"
  }
}
